{
    "name": "iqomp/migrate-mysql",
    "description": "MySQL migration plugin for iqomp/migrate",
    "type": "library",
    "license": "MIT",
    "version": "2.0.0",
    "authors": [
        {
            "name": "Iqbal Fauzi",
            "email": "iqbalfawz@gmail.com"
        }
    ],
    "require": {
        "iqomp/migrate": "^3.0"
    },
    "extra": {
        "hyperf": {
            "config": "Iqomp\\MigrateMysql\\ConfigProvider"
        }
    },
    "autoload": {
        "psr-4": {
            "Iqomp\\MigrateMysql\\": "src/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "composer/composer": "^2.0"
    },
    "scripts": {
        "lint": "./vendor/bin/phpcs --standard=PSR12 src"
    }
}
