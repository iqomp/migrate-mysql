{
    "name": "iqomp/migrate-mysql",
    "description": "MySQL migration plugin for iqomp/migrate",
    "type": "library",
    "license": "MIT",
    "version": "1.0.1",
    "authors": [
        {
            "name": "Iqbal Fauzi",
            "email": "iqbalfawz@gmail.com"
        }
    ],
    "require": {
        "iqomp/migrate": "^2.0"
    },
    "extra": {
        "iqomp/config": "iqomp/config/"
    },
    "autoload": {
        "psr-4": {
            "Iqomp\\MigrateMysql\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iqomp\\MigrateMysql\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "3.*",
        "composer/composer": "^2.0"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests",
        "lint": "./vendor/bin/phpcs --standard=PSR12 src"
    }
}
